{
  "name": "@berlysia/template-matching",
  "version": "1.0.0",
  "license": "MIT",
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "default": "./dist/cjs/index.js"
  },
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "test": "vitest",
    "build": "run-p 'build:*'",
    "build:esm": "tsc --project ./tsconfig.build.json --module es2015 --outDir ./dist/esm",
    "build:cjs": "tsc --project ./tsconfig.build.json --module commonjs --outDir ./dist/cjs",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@berlysia/eslint-config": "^3.2.0",
    "@types/node": "^17.0.31",
    "eslint": "^8.14.0",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4",
    "vitest": "^0.10.5"
  }
}
